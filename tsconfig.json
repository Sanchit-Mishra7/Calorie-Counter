{
    "compilerOptions": {
        "jsx": "react",
        "target": "es2022",
        "experimentalDecorators": true,
        "useDefineForClassFields": false,
        "module": "ESNext",
        "lib": [
        "ES2022",
        "DOM",
        "DOM.Iterable"
    ],
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "allowImportingTsExtensions": true,
    "noEmit": true
    }
}